<h2>@Model.Message_4</h2> 

<form method="post">

    <input type="hidden" asp-for="Organization_Survey.Id" />

    <div class="form-group">
        <label asp-for="Organization_Survey.orgCode"></label>
        <input asp-for="Organization_Survey.orgCode" class="form-control" />
