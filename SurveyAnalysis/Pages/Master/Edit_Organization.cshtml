    <div class="form-group">
        <label asp-for="Organization_Survey.orgName_A"></label>
        <input asp-for="Organization_Survey.orgName_A" class="form-control" />
        <span class="text-danger" asp-validation-for="Organization_Survey.orgName_A"></span>
    </div>

    <div class="form-group">
        <label asp-for="Organization_Survey.partner"></label>
        <input asp-for="Organization_Survey.partner" class="form-control" />
